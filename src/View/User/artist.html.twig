{% extends 'layout.html.twig' %}

{% block title %}Festizik - Les artistes {% endblock %}


{% block content %}
    <div class="container">
        <div class="artist-header">
            <h1>Les artistes</h1>
            <hr>
        </div>

        {% for artist in artists %}
            <div class="artist col-md-3 col-6">
                <h3>{{ artist.name }}</h3>
                <img src="{{ artist.ImageURL }}">
                <p>{{ artist.Description }}</p>
            </div>
        {% endfor %}
    </div>
{% endblock %}